{
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "routes": [
      {
        "src": "/css/(.*)",  
        "dest": "/css/$1"
      },
      {
        "src": "/js/(.*)",  
        "dest": "/js/$1"
      },
      {
        "src": "/images/(.*)",  
        "dest": "/images/$1"
      },
      {
        "src": "/public/(.*)",  
        "dest": "/public/$1"
      },
      {
        "src": "/404.html",
        "dest": "/public/404.html"
